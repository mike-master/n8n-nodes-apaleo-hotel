{
  "name": "n8n-nodes-apaleo-hotel",
  "version": "0.0.3",
  "description": " custom n8n node for interacting with the Apaleo API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "n8n.io",
    "apaleo",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mike-master/n8n-nodes-apaleo-hotel",
  "author": {
    "name": "master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-master/n8n-nodes-apaleo-hotel"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint ./nodes ./credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ApaleoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ApaleoAPI/ApaleoApi.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@typescript-eslint/parser": "~5.45",
    "base-64": "^1.0.0",
    "date-fns": "^4.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
